NODE_ENV=development
PORT=4000

# PostgreSQL connection string
DATABASE_URL=postgres://postgres:postgres@localhost:5432/deneyap_library

# JWT configuration
JWT_SECRET=change-this-secret
JWT_EXPIRES_IN=1d

# Password hashing
PASSWORD_SALT_ROUNDS=10

# Multi-tenant defaults
DEFAULT_TENANT_ID=00000000-0000-0000-0000-000000000001

# Optional settings
PGSSLMODE=disable
SWAGGER_SERVER_URL=
OVERDUE_CRON_SCHEDULE=0 * * * *
CRON_TIMEZONE=UTC
RUN_OVERDUE_ON_BOOT=true

